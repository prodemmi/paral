url = "https://google.com"
filename = "output.txt"

task "scrap" {
    -> @stash["first_stash"] << curl -s https://google.com
    -> @stash["first_stash"] << @replace(@stash("first_stash"), "\n", "")
    -> echo @sprintf(@stash("first_stash"))
}

@depend scrap
task "save" {
    -> echo @sprintf(@stash("first_stash"))
    -> @writefile(filename, @stash("first_stash"))
}
